<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/led5"
    tools:context=".officeappliances">

    <ToggleButton
        android:id="@+id/officetvbtn"
        android:layout_width="98dp"
        android:layout_height="53dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@drawable/led3"
        android:fontFamily="@font/nosifer"
        android:text="ToggleButton"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/officetv"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.152" />

    <TextView
        android:id="@+id/officetv"
        android:layout_width="98dp"
        android:layout_height="53dp"
        android:layout_marginTop="200dp"
        android:fontFamily="@font/wendy_one"
        android:gravity="center"
        android:text="TV"
        android:textColor="#000000"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.236"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.153" />

    <TextView
        android:id="@+id/officeAc"
        android:layout_width="98dp"
        android:layout_height="53dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/wendy_one"
        android:gravity="center"
        android:text="AC"
        android:textColor="#070707"
        android:textSize="36sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.236"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/officetv"
        app:layout_constraintVertical_bias="0.082" />

    <ToggleButton
        android:id="@+id/officeacbtn"
        android:layout_width="98dp"
        android:layout_height="53dp"
        android:layout_marginTop="190dp"
        android:layout_marginBottom="478dp"
        android:background="@drawable/led3"
        android:fontFamily="@font/nosifer"
        android:text="ToggleButton"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.446"
        app:layout_constraintStart_toEndOf="@+id/officeAc"
        app:layout_constraintTop_toBottomOf="@+id/officetvbtn" />

</androidx.constraintlayout.widget.ConstraintLayout>